<?xml version="1.0" encoding="utf-8"?>
<extension>

	<config:android permission="android.permission.VIBRATE"/>
	
	<dependency name="norman-android-extensions" path="dependencies/android" if="android" />
	<android extension="uk.co.mojaworks.norman.Accelerometer" />
	<android extension="uk.co.mojaworks.norman.Vibration" />
	
	<haxelib name="lime" />
	<haxelib name="msignal" />
	<haxelib name="actuate" />
	<haxelib name="hxcpp" if="android" />
	
	<assets path="assets/default" rename="default" />
	
</extension>
